{
    "manifest_version": 3,
    "name": "Ariba PR Helper",
    "version": "0.1",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "description": "Helper for Ariba",
    "permissions": ["tabs", "storage", "activeTab","scripting"],
    "action": {
      
      "default_title": "Ariba PR Helper",
      "default_popup": "popup.html",
      "permissions": ["https://s1-eu.ariba.com/*", "https://proview-ui.prod.daaa.cloud/*", "scripting" ,"storage"]
    },
    "content_scripts": [
      {
        "matches": ["https://s1-eu.ariba.com/*", "https://proview-ui.prod.daaa.cloud/*"],
        "js": ["content.js"]
      }
    ]
  }
  